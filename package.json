		"CSR",
		"greenlock",
		"VanillaJS",
		"ZeroSSL"
	],
	"author": "AJ ONeal <coolaj86@gmail.com> (https://coolaj86.com/)",
	"license": "MPL-2.0",
	"dependencies": {
		"@root/csr": "^1.0.0-wip.0",
		"@root/encoding": "^1.0.1",
		"@root/keypairs": "^1.0.0-wip.0"
	},
	"devDependencies": {
		"@root/request": "^1.3.10",
		"dig.js": "^1.3.9",
		"dns-suite": "^1.2.12",
		"dotenv": "^8.1.0",
		"punycode": "^1.4.1"
	}
}
